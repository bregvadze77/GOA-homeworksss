
for i in range(1,11):
    print("hello world")
    
sum = 0
for i in range(1,11):
    sum =+i
print(sum)

for i in range(1,1000,100):
    print(i)

for i in range(100,-1,-1):
    print(i)

i = 1
while i <=10:
  print("hello world")
  i+=1

  sum = 0
  i = 1

while i <= 10:
    sum =+i
    i+=1
    
i = 0
while i <=1000:
    print(i)
    i+=100

i = 100
while i >=0:
    print(i)
    i-=1


